class Solution {
      public int[] twoSum(int[] n, int tar)
     // n=n.length;
       {
       // int leg=n.length();
        for (int i = 0; i < n.length; i++) {
        for (int j = i + 1; j < n.length; j++) {
            if (n[j] == tar - n[i]) {
                return new int[] { i, j };
            }
        }
    }
    throw new IllegalArgumentException("No two sum solution");
   // throw new NotAcceptedException("No two sum solution");
        
    }
}
