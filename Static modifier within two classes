//class x, static method call in main class Demo6
class x {
    static void fun() {
        System.out.println("hello..");
    }
}

class Demo6 {
    static void disp() {
        x.fun();
        System.out.println("HIII..");
    }

    public static void main(String a[]) {
        Demo6 ob = new Demo6();
        ob.disp();
        // .ob1 = new x();
        // ob1.fun();
    }
}
